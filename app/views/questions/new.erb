<div class="question-container">
  <h1> New Question </h1>
<form method="post" action="/questions">
  <input type="text" name="question[title]" placeholder="Title"><br>
  <input type="text" name="question[body]" placeholder="Body"><br>
  <input type="submit" value="Ask!">
</form>
<br>
<% if @errors %>
  <%= erb :'_errors' %>
<% end %>

</div>
